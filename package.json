{
  "name": "corevia-fitness-tracking",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:ts": "tsc && vite build",
    "build:github": "cross-env GITHUB_PAGES=true vite build",
    "postinstall": "npm dedupe",
    "preview": "vite preview",
    "predeploy": "npm run build:github",
    "deploy": "gh-pages -d dist",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "5.76.1",
    "chart.js": "^4.4.0",
    "date-fns": "^4.1.0",
    "firebase": "^10.14.1",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.507.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.0",
    "uuid": "11.1.0",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/firebase": "^2.4.32",
    "@types/node": "^22.15.3",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "4.3.9",
    "vitest": "^0.34.6"
  },
  "homepage": "https://coreoffitness2025.github.io/Corevia-Fitness-Tracking-Ver-2"
}
